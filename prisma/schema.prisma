generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Application {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  name            String
  whatsapp        String
  mainChallenge   String
  reactionToBlock String
  controlLevel    Int
  finalFit        ApplicationFit
  notJoinReason   String?
}

enum ApplicationFit {
  YES
  NO
}
